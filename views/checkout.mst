    });
	
  </script>

<script type="text/javascript">
/*global $:false */
$(document).ready(function(){"use strict";
	$(".scroll").click(function(event){

		event.preventDefault();

		var full_url = this.href;
		var parts = full_url.split("#");
		var trgt = parts[1];
		var target_offset = $("#"+trgt).offset();
		var target_top = target_offset.top;

		$('html, body').animate({scrollTop:target_top}, 1200);
	});
});

$(document).ready(() => {    
    const amountInput = document.querySelector('#purchase_amount')
    const amountLabel = document.querySelector('#total_purchase_label')

    const setTotal= (quantity) => {
        amountLabel.innerHTML =  `Total: $${quantity * {{price}}}.00 MXN`
    }

    setTotal(amountInput.value)

    amountInput.addEventListener('change', (ev) => {
        setTotal(ev.target.value)
        
    })
})
</script>
